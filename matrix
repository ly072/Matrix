#include<iostream>
#include<cstdlib>

using namespace std;

void transpose(int hang,int lie,int **a)
{
	for (int i = 0; i < hang; i++)
	{
		for (int j = 0; i < lie; j++)
		{
			cin >> a[i][j];
		}
	}
}

